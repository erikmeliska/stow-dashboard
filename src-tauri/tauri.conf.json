{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "Stow Dashboard",
  "version": "0.1.0",
  "identifier": "com.trisoft.stow-dashboard",
  "build": {
    "beforeBuildCommand": "node scripts/prepare-tauri.mjs"
  },
  "app": {
    "withGlobalTauri": false,
    "windows": [],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": ["app", "dmg"],
    "resources": {
      "../.next/standalone": "standalone"
    },
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "macOS": {
      "minimumSystemVersion": "10.15"
    }
  }
}
